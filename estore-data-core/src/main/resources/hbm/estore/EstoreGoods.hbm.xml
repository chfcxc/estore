<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.emay.estore.pojo.estore.EstoreGoods" table="estore_goods">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="storeId" column="store_id" type="long" />
		<property name="sortNum" column="sort_num" type="integer" />
		<property name="name" column="name" type="string" />
		<property name="goodsDescribe" column="goods_describe" type="string" />
		<property name="price" column="price" type="big_decimal" />
		<property name="discount" column="discount" type="big_decimal" />
		<property name="discountPrice" column="discount_price" type="big_decimal" />		
		<property name="createTime" column="create_time" type="timestamp" />
		<property name="isDelete" column="is_delete" type="boolean" />
	</class>
</hibernate-mapping>
