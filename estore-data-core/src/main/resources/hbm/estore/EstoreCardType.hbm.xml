<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.emay.estore.pojo.estore.EstoreCardType" table="estore_card_type">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="level" column="level" type="int" />
		<property name="storeId" column="store_id" type="long" />
		<property name="name" column="name" type="string" />
		<property name="cardDescribe" column="card_describe" type="string" />
		<property name="createTime" column="create_time" type="timestamp" />
	</class>
</hibernate-mapping>
