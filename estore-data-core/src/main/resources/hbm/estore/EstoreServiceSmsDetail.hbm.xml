<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.emay.estore.pojo.estore.EstoreServiceSmsDetail" table="estore_service_sms_detail">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="batchId" column="batch_id" type="long" />
		<property name="userCustomerId" column="user_customer_id" type="long" />
		<property name="mobile" column="mobile" type="string" />
		<property name="customerId" column="customer_id" type="string" />
		<property name="responseCode" column="response_code" type="string" />	
		<property name="responseMessage" column="response_message" type="string" />		
		<property name="sendTime" column="send_time" type="timestamp" />	
		<property name="state" column="state" type="int" />		
		<property name="createTime" column="create_time" type="timestamp" />
	</class>
</hibernate-mapping>
