<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.emay.estore.pojo.system.AuthOper" table="system_auth_oper">
		<id name="id" column="id">
			<generator class="native" />
		</id>
		<property name="authCode" column="auth_code" type="string" />
		<property name="index" column="l_index" type="integer" />
		<property name="name" column="name" type="string" />
		<property name="pageAuthId" column="page_auth_id" type="long" />
		
	</class>
</hibernate-mapping>
